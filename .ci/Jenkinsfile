pipeline {
    agent any
    options {
        ansiColor('xterm')
        buildDiscarder(logRotator(numToKeepStr: '5', artifactNumToKeepStr: '5'))
    }

    stages {

        stage('Log variables') {
            when {
                allOf {
                    expression { env.STACKABLE_OPERATOR != null }
                }
            }
            steps {
                echo "Building PR #${env.STACKABLE_OPERATOR}"
            }
        }

    }
}


